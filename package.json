{
  "name": "obsidian-mdx",
  "version": "0.1.0",
  "description": "Preview MDX in Obsidian, with support for CodeHike",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "mdx",
    "obsidian",
    "codehike"
  ],
  "author": "Yulei Chen",
  "repository": "https://github.com/yuleicul/obsidian-mdx",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.57",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@code-hike/mdx": "^0.8.3",
    "@mdx-js/mdx": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shiki": "^0.14.2"
  }
}
